JS игра по мотивам ЗВ:
<meta charset="utf-8">
<script>

alert("Смирно кадет. Перед тобой Адмирал Джиал Акбар: \"Поздравляю. Теперь вы служите наводчиком главной артелирийской баттареи тяжелого крейсера типа Бесстрашный.\nВам нужно правильно выбрать угол атаки для поражения 2х Имперских крейсеров типа Аккламатор I.\nЗначение угла необходимо задавать числом от 0 до 90\" ");

  let shot = "";
  let score = 0;

  shot = +prompt("Мастер-Джедай Йода:\"Угол верный выберите вы для выстрела первого\" ");
    if (isNaN (shot)) {
      alert("Мастер-Джедай Йода: \"Не число указали Вы!\" ");
  } else if ((shot<0 || shot>90)) {
      alert("Мастер-Джедай Йода: \"Диапазон не допустимый здесь указан\" ");
  }
    if ((shot>=5 && shot<=30)) {
      alert("Мастер-Джейдай Йода: \"Браво! Первый крейсер поразили Вы! В духе том же продолжайте. Падет Империи блокада вскоре.\" ");
    score = score+1;
  }
    else if ((shot>=65 && shot<=90)) {
      alert("Мастер-Джейдай Йода: \"Сила сегодня нам  помогает. Уничтожен был второй Крейсер.\" ");
      score = score+1;
  }

  shot = +prompt("Мастер-Джедай Йода: \"Цель вторую поразить нам надо,нельзя ошибку допустить\" ");
    if (isNaN (shot)) {
      alert("Не число указали Вы!");
}   else if ((shot<0 || shot>90)) {
      alert("Мастер-Джедай Йода: \"Диапазон не допустимый здесь указан\" ");
}
    if ((shot>=5 && shot<=30)) {
      alert("Мастер-Джейдай Йода: \"Браво! Первый крейсер поразили Вы! В духе том же продолжайте. Падет Империи блокада вскоре.\" ");
        score = score+1;
}
    else if ((shot>=65 && shot<=90)) {
      alert("Мастер-Джейдай Йода: \"Сила помогает нам сегодня. Уничтожен был второй Крейсер.\" ");
      score = score+1;
}
    if (score == 2) {
      alert("Мастер-Джедай Йода: \"Блокаду Империи успешно прорвали Мы, славная победа сегодня для Альянса!\" ")
}
    else if (score == 1) {
      alert("Мастер-Джейдай Йода: \"Один Крейсер повержен был... Отступить придется нам.\" ")
}
    else {
      alert ("Мастер-Джедай Йода: \"Промахнулись Вы. Надежды не осталось больше!\" ");
  }
      alert ("Вы поразили целей " + score);
</script>
